<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <security_settings_path>
        <route>security/settings/*</route>
        <defaults>
            <module>security</module>
            <controller>settings</controller>
            <action>index</action>
        </defaults>
    </security_settings_path>
    <new_security_session_path>
        <route>session/new/*</route>
        <defaults>
            <module>security</module>
            <controller>sessions</controller>
            <action>new</action>
        </defaults>
    </new_security_session_path>
    <create_security_session_path>
        <route>session/create/*</route>
        <defaults>
            <module>security</module>
            <controller>sessions</controller>
            <action>create</action>
        </defaults>
    </create_security_session_path>
    <delete_security_session_path>
        <route>session/delete/*</route>
        <defaults>
            <module>security</module>
            <controller>sessions</controller>
            <action>delete</action>
        </defaults>
    </delete_security_session_path>
    <destroy_security_session_path>
        <route>session/destroy/*</route>
        <defaults>
            <module>security</module>
            <controller>sessions</controller>
            <action>destroy</action>
        </defaults>
    </destroy_security_session_path>
    <security_account_path>
        <route>security/accounts/:id/*</route>
        <defaults>
            <module>security</module>
            <controller>accounts</controller>
            <action>show</action>
        </defaults>
        <reqs>
            <id>\d+</id>
        </reqs>
    </security_account_path>
    <security_accounts_path>
        <route>security/accounts/*</route>
        <defaults>
            <module>security</module>
            <controller>accounts</controller>
            <action>index</action>
        </defaults>
    </security_accounts_path>
    <new_security_account_path>
        <route>security/accounts/new/*</route>
        <defaults>
            <module>security</module>
            <controller>accounts</controller>
            <action>new</action>
        </defaults>
    </new_security_account_path>
    <edit_security_account_path>
        <route>security/accounts/:id/edit/*</route>
        <defaults>
            <module>security</module>
            <controller>accounts</controller>
            <action>edit</action>
        </defaults>
        <reqs>
            <id>\d+</id>
        </reqs>
    </edit_security_account_path>
    <delete_security_account_path>
        <route>security/accounts/:id/delete/*</route>
        <defaults>
            <module>security</module>
            <controller>accounts</controller>
            <action>delete</action>
        </defaults>
        <reqs>
            <id>\d+</id>
        </reqs>
    </delete_security_account_path>
    <security_group_path>
        <route>security/groups/:id/*</route>
        <defaults>
            <module>security</module>
            <controller>groups</controller>
            <action>show</action>
        </defaults>
        <reqs>
            <id>\d+</id>
        </reqs>
    </security_group_path>
    <security_groups_path>
        <route>security/groups/*</route>
        <defaults>
            <module>security</module>
            <controller>groups</controller>
            <action>index</action>
        </defaults>
    </security_groups_path>
    <new_security_group_path>
        <route>security/groups/new/*</route>
        <defaults>
            <module>security</module>
            <controller>groups</controller>
            <action>new</action>
        </defaults>
    </new_security_group_path>
    <edit_security_group_path>
        <route>security/groups/:id/edit/*</route>
        <defaults>
            <module>security</module>
            <controller>groups</controller>
            <action>edit</action>
        </defaults>
        <reqs>
            <id>\d+</id>
        </reqs>
    </edit_security_group_path>
    <delete_security_group_path>
        <route>security/groups/:id/delete/*</route>
        <defaults>
            <module>security</module>
            <controller>groups</controller>
            <action>delete</action>
        </defaults>
        <reqs>
            <id>\d+</id>
        </reqs>
    </delete_security_group_path>
</routes>